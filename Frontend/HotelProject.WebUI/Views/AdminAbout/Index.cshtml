@model List<ResultAboutDto>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/AdminLayout/_AdminLayout.cshtml";
}

<table class="table">
    <thead>
        <tr>
            <th>#</th>
            <th>Title1</th>
            <th>Title2</th>
            <th>Created Date</th>
            <th>Update</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.id</td>
                <td>@item.Title1</td>
                <td>@item.Title2</td>
                <td>@item.CreatedDate</td>
                <td><a href="/adminabout/updateAbout/@item.id" class="btn mb-1 btn-rounded btn-outline-success">Update</a></td>
            </tr>
        }
    </tbody>
</table>
